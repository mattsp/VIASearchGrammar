  "version": "0.2.0",
  "configurations": [
    {
      "type": "antlr-debug",
      "request": "launch",
      "name": "Run Parser",
      "input": "searchQuery.txt",
      "grammar": "VIASearchParser.g4",
      "startRule": "query",
      "printParseTree": true,
      "visualParseTree": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Lancer le programme",
      "program": "${workspaceFolder}\\index.js"
    }
  ]
}
